# Required API Keys
# Get your Infura API key at https://app.infura.io
NEXT_PUBLIC_INFURA_API_KEY=your_infura_key_here

# Get your WalletConnect Project ID at https://cloud.walletconnect.com
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Contract Addresses
# For development: Example hardhat addresses (replace with your deployed contract addresses)
NEXT_PUBLIC_TOKEN_ADDRESS=
NEXT_PUBLIC_LP_TOKEN_ADDRESS=
NEXT_PUBLIC_STAKING_REWARDS_ADDRESS=

# Network Configuration
# 8453 = Base Mainnet
# 84531 = Base Testnet (Sepolia)
NEXT_PUBLIC_CHAIN_ID=8453